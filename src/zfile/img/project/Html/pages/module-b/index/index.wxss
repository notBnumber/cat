.container {
    min-height: 100vh;
    background-color: #f3f4f6;
}

.swiper {
    width: 90vw;
    height: 45vw;
    margin: 5vw;
    border-radius: 2vw;
    overflow: hidden;
    background-color: #fff;
    box-shadow: 0 0 5rpx #e1dde5;
}

.swiper .swiper-item {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
}

.swiper .ads-img {
    width: 90vw;
    height: 45vw;
    display: block;
}

.swiper .top {
    width: 100%;
    height: 110rpx;
    /* background-color: #906ba1; */
    position: relative;
}

.swiper .top .hospital {
    font-size: 30rpx;
    color: #fff;
    display: inline-block;
    margin: 35rpx;
    letter-spacing: 2rpx;/*字间距*/
    width: 480rpx;/*指定宽度*/
    word-break: keep-all;/* 不换行 */
    white-space: nowrap;/* 强制在同一行内显示所有文本,直到文本结束或者遭遇 br 对象。不换行 */
    overflow: hidden;/* 内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis;/* IE 专有属性,当对象内文本溢出时显示省略标记(...) ;需与overflow:hidden;一起使用。*/
}

.swiper .top .tips {
    font-size: 20rpx;
    /* color: #652e81; */
    display: inline-block;
    padding: 1px 60rpx 1px 20rpx;
    background-color: #fff;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
    position: absolute;
    right: 0;
    top: 40rpx;
}

.swiper .bom {
    position: relative;
}

.swiper .bom .doctor {
    font-size: 30rpx;
    display: inline-block;
    margin: 50rpx 40rpx;
}

.swiper .bom .progress_box {
    width: 170rpx;
    height: 170rpx;
    position: absolute;
    right: 30rpx;
    top: 20rpx;
}

.swiper .progress_bg {
    position: absolute;
    width: 170rpx;
    height: 170rpx;
}

.swiper .progress_canvas {
    width: 170rpx;
    height: 170rpx;
}

.swiper .progress_text {
    width: 120rpx;
    position: absolute;
    top: 32rpx;
    right: 25rpx;
    text-align: center;
}

.swiper .progress_text text {
    display: block;
    width: 120rpx;
}

.swiper .progress_info {
    font-size: 22rpx;
}

.options {
    margin: 0 40rpx;
}

.options .tab {
    width: 200rpx;
    font-weight: 600;
    background-color: #fff;
    border: 1rpx solid #fff;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.options .tab .tab-cn {
    font-size: 35rpx;
    display: inline-block;
    margin-left: 18rpx;
    margin-right: 10rpx;
}

.options .tab .tab-en {
    font-size: 25rpx;
    display: inline-block;
}

.options .options-list {
    background-color: #fff;
    border: 1rpx solid #fff;
    padding-bottom: 40rpx;
    border-radius: 0 20rpx 20rpx 20rpx;
}

.options .options-list .spe {
    width: calc(100% - 65rpx);
    height: 180rpx;
    border: 1rpx solid #fff;
    border-radius: 20rpx;
    color: #fff;
    margin-left: 30rpx;
    margin-top: 40rpx;
    position: relative;
}

.options .options-list .spe .spe-cn {
    width: 180rpx;
    font-size: 50rpx;
    margin-left: 100rpx;
    margin-top: 40rpx;
}

.options .options-list .spe .spe-en {
    width: 180rpx;
    font-size: 25rpx;
    margin-left: 100rpx;
    margin-top: 5rpx;
}

.options .options-list .spe .spe-ico {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    right: 80rpx;
    top: 40rpx;
}

.options .options-list .com {
    width: calc((100vw - 180rpx) / 2);
    height: 180rpx;
    border: 1rpx solid #fff;
    display: inline-block;
    color: #fff;
    position: relative;
    border-radius: 20rpx;
    margin-left: 30rpx;
    margin-top: 40rpx;
    overflow: hidden;
}

.com-bg {
    width: 100%;
    height: 100%;
    z-index: 0;
    position: absolute;
}

.com-float {
    width: 100%;
    height: 100%;
    z-index: 1;
    position: absolute;
}

.options .options-list .com .com-cn {
    font-size: 30rpx;
    margin-left: 30rpx;
    margin-top: 40rpx;
}

.options .options-list .com .com-en {
    font-size: 20rpx;
    margin-left: 30rpx;
    margin-top: 10rpx;
}

.options .options-list .com .com-ico {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: 30rpx;
    top: 50rpx;

}